<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open in Terminal</name>
	<submenu></submenu>
	<unique-id>1738235439562558-1</unique-id>
	<command>xdg-terminal-exec -- --working-directory %f</command>
	<description>Launch terminal with this folder as working directory</description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>preferences-desktop-wallpaper</icon>
	<name>Set as Wallpaper (swww)</name>
	<submenu></submenu>
	<unique-id>1738407598793718-1</unique-id>
	<command>swww img %f</command>
	<description>Set desktop wallpaper using swww</description>
	<range>*</range>
	<patterns>*</patterns>
	<image-files/>
</action>
<action>
	<icon>administration</icon>
	<name>Open as Admin</name>
	<submenu></submenu>
	<unique-id>1740647023068646-1</unique-id>
	<command>sudo -A -E thunar %f</command>
	<description>Open Thunar as root user</description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
</action>
</actions>
